    "mustache": "^2.3.0",
    "natives": "^1.1.6",
    "nodemon": "^1.19.3",
    "ramda": "^0.26.1",
    "react": "^16.2.0",
    "react-dom": "^16.14.0",
    "watch": "^1.0.2"
