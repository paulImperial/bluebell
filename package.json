{
  "name": "bluebell",
  "version": "1.0.0",
  "description": "Bluebell therapy website",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm lint:js"
    }
  },
  "scripts": {
    "prebuild": "del-cli dist -f",
