    "eslint": "^4.18.12",
    "eslint-config-google": "^0.9.1",
    "express": "^4.14.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^3.0.3",
    "gulp-foreach": "^0.1.0",
    "gulp-html-minifier": "^0.1.8",
    "gulp-mustache-plus": "^1.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-uncss": "^1.0.6",
    "gulp-watch": "^4.3.11",
    "gulp4-run-sequence": "^1.0.0",
